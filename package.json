{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "fix": "yarn fix:eslint && yarn fix:prettier && yarn fix:stylelint",
    "fix:eslint": "eslint **/*.{js,ts,vue} --fix",
    "fix:prettier": "prettier --write **/*.{vue,js,ts,css,sass,scss}",
    "fix:stylelint": "stylelint --fix **/*.{vue,css,sass,scss} --ignore-path",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "lint:eslint": "eslint **/*.{js,ts,vue}",
    "lint:prettier": "prettier --check **/*.{vue,js,ts,css,sass,scss}",
    "lint:stylelint": "stylelint **/*.{vue,css,sass,scss} --ignore-path",
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "buildandstart": "cross-env NODE_ENV=production nuxt build && cross-env NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "test": "jest --setupFiles \"dotenv/config\""
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix",
    "*.{vue,js,ts,css,sass,scss}": "prettier --write",
    "*.{vue,css,sass,scss}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "core-js": "^3.9.1",
    "intersection-observer": "^0.12.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.3",
    "nuxt-i18n": "^6.27.2",
    "nuxt-webfontloader": "^1.1.0",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-lazyload": "^1.3.3",
    "vue-svg-loader": "^0.16.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^2.0.10",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "cssnano-preset-advanced": "^5.1.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^4.3.8",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-sass": "^6.0.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.4",
    "vue-jest": "^3.0.4"
  }
}
